{
  "name": "root",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@size-limit/preset-small-lib": "^4.9.1",
    "@storybook/addon-docs": "6.1.21",
    "@storybook/addon-essentials": "^6.1.18",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.1.18",
    "@storybook/addons": "^6.1.18",
    "@storybook/react": "^6.1.18",
    "@testing-library/react": "^11.2.5",
    "@types/lodash": "^4.14.168",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-router-dom": "^5.1.7",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "customize-cra": "^1.0.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^4.3.7",
    "lerna": "^4.0.0",
    "prettier": "^2.2.1",
    "react-app-rewired": "^2.1.8",
    "react-scripts": "^4.0.3",
    "regenerator-runtime": "^0.13.7",
    "size-limit": "^4.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typedoc": "^0.20.19",
    "typedoc-plugin-markdown": "^3.4.5",
    "typescript": "^4.1.3",
    "webpack": "^5.26.3"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "lerna": "lerna",
    "start": "lerna run start --stream --parallel",
    "test": "lerna run test --",
    "test:changed": "lerna run --since origin/master --include-dependents test --",
    "test:update": "lerna run test -- -u",
    "test:update:changed": "lerna run --since origin/master --include-dependents test -- -u",
    "typecheck": "lerna run tsc --noEmit",
    "typecheck:changed": "lerna run --since origin/master --include-dependents tsc --noEmit",
    "lint": "lerna run lint -- --fix",
    "lint:changed": "lerna run --since origin/master --include-dependents lint -- --fix",
    "build": "lerna run build",
    "build:changed": "lerna run --since origin/master --include-dependents build",
    "prepublish": "lerna run prepublish",
    "start:app": "yarn run build && yarn --cwd example && yarn --cwd example start",
    "docs": "lerna run docs --",
    "cypress": "cypress run",
    "cypress:open": "cypress open",
    "changeset": "changeset",
    "release": "changeset publish",
    "version:canary": "changeset version --snapshot canary",
    "release:canary": "changeset publish --tag canary"
  },
  "dependencies": {
    "@ballware/react-contexts": "^6.2.0-alpha2",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "bootstrap-icons": "^1.4.0",
    "devextreme": "^20.2.5",
    "devextreme-react": "^20.2.5",
    "i18next": "^19.9.2",
    "jquery": "^3.6.0",
    "json5": "^2.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "moment-duration-format": "^2.3.2",
    "oidc-client": "^1.11.5",
    "popper.js": "^1.16.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.8.10",
    "react-router-dom": "^5.2.0",
    "uuid": "^8.3.2"
  }
}
