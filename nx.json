  "projects": {
    "ballware-webclient": { "tags": [] },
    "ballware-webclient-e2e": {
      "tags": [],
      "implicitDependencies": ["ballware-webclient"]
    },
    "identity-interface": { "tags": [] },
    "identity-backend": { "tags": [] },
    "meta-interface": { "tags": [] },
    "meta-backend": { "tags": [] },
    "react-contexts": { "tags": [] },
    "react-provider": { "tags": [] },
    "react-renderer": { "tags": [] },
    "react-renderer-dx": { "tags": [] },
    "react-renderer-dx-e2e": {
      "tags": [],
      "implicitDependencies": ["react-renderer-dx"]
    },
    "react-renderer-bs": { "tags": [] }
  }
}
