{
  "name": "@ballware/react-provider",
  "version": "6.1.0-alpha.2",
  "description": "Ballware Application Builder - React provider for contexts",
  "license": "MIT",
  "author": "frank@ballmeyer.com",
  "homepage": "https://github.com/ballware/ballware-client",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ballware/ballware-client.git",
    "directory": "packages/react-provider"
  },
  "bugs": {
    "url": "https://github.com/ballware/ballware-client/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint src --fix",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "docs": "typedoc --plugin typedoc-plugin-markdown"
  },
  "peerDependencies": {
    "oidc-client": ">=1.11.5",
    "axios": ">=0.21.1",
    "lodash": ">=4.17.20",
    "moment": ">=2.29.1",
    "uuid": ">=8.3.2",
    "react": ">=16",
    "react-router-dom": ">=5.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/ballware-react-provider.esm.js",
  "size-limit": [
    {
      "path": "dist/ballware-react-provider.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/ballware-react-provider.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "oidc-client": "^1.11.5",
    "axios": "^0.21.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "@ballware/identity-interface": "^6.1.0-alpha.2",
    "@ballware/meta-interface": "^6.1.0-alpha.2",
    "@ballware/react-contexts": "^6.1.0-alpha.2"
  }
}
